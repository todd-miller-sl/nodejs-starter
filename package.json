{
  "name": "nodejs-starter",
  "version": "0.0.1",
  "private": false,
  "description": "Node.js starter template",
  "keywords": [
    "javascript",
    "nodejs",
    "starter",
    "express",
    "knexjs",
    "postgresql"
  ],
  "author": {
    "name": "Dustin",
    "email": "dustinmwang2104@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github-dustin:dustinmwang2104/nodejs-starter.git"
  },
  "homepage": "https://github.com/dustinmwang2104/nodejs-starter",
  "bugs": {
    "url": "https://github.com/dustinmwang2104/nodejs-starter/issues"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "npm install --production",
    "start": "node .",
    "dev": "nodemon",
    "test": "NODE_ENV=testing mocha",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:refresh": "npm run unmigrate && npm run migrate",
    "migrate:refresh:seed": "npm run migrate:refresh && npm run seed",
    "unmigrate": "knex migrate:rollback",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.1",
    "knex": "^0.95.14",
    "pg": "^8.7.1",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.0",
    "should": "^13.2.3",
    "sinon": "^12.0.1",
    "supertest": "^6.1.6"
  }
}
